/dts-v1/;
#include "mt7986b.dtsi"

/ {
	model = "MediaTek MT7986b RFB";
	compatible = "mediatek,mt7986b-rfb";

	aliases {
		serial0 = &uart0;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	memory@40000000 {
		device_type = "memory";
		reg = <0 0x40000000 0 0x40000000>;
	};
};

&uart0 {
	status = "okay";
};
