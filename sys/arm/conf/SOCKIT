#NO_UNIVERSE

include 	"SOCFPGA"
ident		SOCKIT

options 	ROOTDEVNAME=\"ufs:/dev/mmcsd0s4\"

# Flattened Device Tree
options 	FDT_DTB_STATIC
makeoptions	FDT_DTS_FILE=socfpga_cyclone5_sockit_sdmmc.dts
